<!DOCTYPE html>
<html lang="en">
@{ await Html.RenderPartialAsync("_Head");}
<body class="my-0">
    @{ await Html.RenderPartialAsync("_DashHeader");}
    <div class="container">
        <div class="row">
            <div class="col-md-4">
                @{ await Html.RenderPartialAsync("_DashSidebar");}
            </div>
            <div class="col-md-8" style="height:600px">
                @RenderBody()
            </div>
        </div>
    </div>
    @{ await Html.RenderPartialAsync("_Footer");}
    @{ await Html.RenderPartialAsync("_Scripts");}
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

